<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DefineConstants>$(DefineConstants);HSGE</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>$(DefineConstants);HSGE</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="UnityTranspiler\**" />
	  <EmbeddedResource Remove="UnityTranspiler\**" />
	  <None Remove="UnityTranspiler\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Coroutine" Version="2.1.5" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="Websocket.Client" Version="5.2.0" />
	</ItemGroup>


</Project>
